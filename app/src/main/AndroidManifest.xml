<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.proyectofinal">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.orm.SugarApp"
        android:allowBackup="true"
        android:icon="@mipmap/icon_restaurant"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/icon_restaurant_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProyectoFinal">
        <activity
            android:name=".Lista_Reserva"
            android:exported="false" />
        <activity
            android:name=".Reserva_Detalle"
            android:exported="false" />
        <activity
            android:name=".form_delivery"
            android:exported="false" />
        <activity
            android:name=".form"
            android:exported="false" />
        <activity
            android:name=".ListaPlatos"
            android:exported="false" />
        <activity
            android:name=".PedidoCarrito"
            android:exported="false" />
        <activity
            android:name=".plato_detalle"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="DATABASE"
            android:value="sugarormapp.db" />
        <meta-data
            android:name="VERSION"
            android:value="1" />
        <meta-data
            android:name="QUERY_LOG"
            android:value="true" />
        <meta-data
            android:name="DOMAIN_PACKAGE_NAME"
            android:value="com.miexample.proyectofinal.orm" />
    </application>

</manifest>